{
  "name": "@garrigarrison/datetime",
  "private": false,
  "version": "0.0.6",
  "description": "converter DateTime into timestamp and revert",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarriGarrison/datetime.git"
  },
  "author": {
    "name": "Garrison",
    "email": "garrisonford@yandex.ru",
    "url": "https://github.com/GarriGarrison"
  },
  "homepage": "https://github.com/GarriGarrison/datetime#readme",
  "bugs": {
    "url": "https://github.com/GarriGarrison/datetime/issues"
  },
  "main": "index.js",
  "module": "./es/index.js",
  "types": "./lib/index.d.ts",
  "jsnext:main": "./es/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:commonjs && npm run build:es",
    "build:commonjs": "rimraf lib && tsc --downlevelIteration --target es5 --outDir dist/lib --module commonjs --declaration",
    "build:es": "rimraf es && tsc --target es6 --outDir dist/es --module es6",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "date",
    "time",
    "datetime"
  ],
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "types-spring": "^2.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "sideEffects": false
}
